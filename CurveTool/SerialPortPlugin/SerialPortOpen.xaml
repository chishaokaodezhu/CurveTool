<Window x:Class="SerialPortPlugin.SerialPortOpen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SerialPortPlugin"
        mc:Ignorable="d"
        Title="SerialPortOpen" Height="225" Width="485.447" ResizeMode="NoResize">
    <Grid>
        <ComboBox x:Name="portList" HorizontalAlignment="Left" Margin="23,73,0,0" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="baud" HorizontalAlignment="Left" Height="23" Margin="201,71,0,0" TextWrapping="Wrap" Text="9600" VerticalAlignment="Top" Width="86"/>
        <Button Content="确定" HorizontalAlignment="Left" Margin="352,70,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
